{
  "name": "react-jalali-datepicker",
  "version": "0.1.7",
  "private": false,
  "description": "a Simple and Flexible Datepicker for react and react-native",
  "main": "./dist/index.js",
  "scripts": {
    "clear": "rimraf ./dist/*",
    "build:dev": "webpack-dev-server -w -d --progress",
    "build:prod": "npm run clear && webpack -p --progress",
    "build:publish": "rm -rf dist && mkdir dist && babel ./src/js/Components/Datepicker -d dist --copy-files"
  },
  "author": "Ahmad Mirzaei (@a_m_dev)",
  "license": "ISC",
  "keywords": [
    "datepicker",
    "calendar",
    "react",
    "react-native",
    "react-hooks",
    "jalali"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/a-m-dev/react-jalali-datepicker.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.2.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.14.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "moment-jalaali": "^0.9.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
